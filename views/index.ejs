<!DOCTYPE html>
<html dir="ltr" lang="en-US">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

    <!-- Stylesheets
	============================================= -->
    <%- include("partials/css") %>
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Document Title
	============================================= -->
    <title>Home | Syticks</title>
  </head>

  <body class="stretched">
    <!-- Document Wrapper
	============================================= -->
    <!-- Top Bar
		============================================= -->
    <%- include("partials/top") %>
    <!-- #top-bar end -->

    <div id="wrapper" class="clearfix">
      <!-- Header
		============================================= -->
      <%- include("partials/header") %>
      <!-- #header end -->

      <!-- hero section -->
      <section
        id="slider"
        class="slider-element slider-parallax swiper_wrapper full-screen clearfix"
        data-autoplay="7000"
        data-speed="3000"
        data-loop="true"
      >
        <div class="slider-parallax-inner">
          <div class="swiper-container swiper-parent">
            <div class="swiper-wrapper">
              <div
                class="swiper-slide dark"
                style="
                  background-image: url('./images/custom/welcome.jpg');
                "
              ></div>
              <div
                class="swiper-slide dark"
                style="
                  background-image: url('./images/custom/tickets.jpg');
                "
              ></div>

              <div
                class="swiper-slide dark"
                style="
                  background-image: url('./images/custom/enquires.jpg');
                "
              ></div>
            </div>
            <div class="slider-arrow-left"><i class="icon-angle-left"></i></div>
            <div class="slider-arrow-right">
              <i class="icon-angle-right"></i>
            </div>
            <div class="slide-number">
              <div class="slide-number-current"></div>
              <span>/</span>
              <div class="slide-number-total"></div>
            </div>
            <div class="swiper-pagination"></div>
          </div>
        </div>
      </section>

      <section id="content">
        <!-- /hero section -->
        <div class="fancy-title title-border title-center topmargin-sm">
          <h4> Upcoming Events</h4>
          <a href="#">[See More]</a>
        </div>
        <div class="content-wrap">
          <div class="container clearfix">
            <!-- Shop
            ============================================= -->
            <div
              id="shop"
              class="shop grid-container clearfix"
              data-layout="fitRows"
            >


<% for (let i = 0; i < events.length; i++) {%>
     <div class="product clearfix">
                <div class="product-image">
                  <a href="#"
                    ><img
                      src="images/shop/dress/1.jpg"
                      alt="Checked Short Dress"
                  /></a>

                 
                </div>
                <div class="product-desc">
                  <div class="product-title">
                    <h3><a href="/events/<%=events[i].slug%>"><%= events[i].name%></a></h3>
                  </div>
                 
                </div>
              </div>
  
<% } %>

           
             
            </div>
            <!-- #shop end -->
          </div>
        </div>
      </section>
     

      <!-- Content=======   -->

      <!-- #content end -->

      <!-- Footer
		============================================= -->
      <%- include('partials/footer') %>
      <!-- #footer end -->

      <!-- #footer end -->
    </div>
    <!-- #wrapper end -->

    <!-- Go To Top
	============================================= -->
    <div id="gotoTop" class="icon-angle-up"></div>

    <!-- External JavaScripts
	============================================= -->
    <script src="js/jquery.js"></script>
    <script src="js/plugins.js"></script>

    <!-- Footer Scripts
	============================================= -->
    <script src="js/functions.js"></script>
  </body>
</html>
